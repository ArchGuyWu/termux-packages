type = process
command = $PREFIX/bin/rabbitmq-server $RABBITMQ_CONFIG_FILE
env-file = config/rabbitmq-server.env
restart = on-failure
smooth-recovery = yes
depends-on: network.target
logfile = $PREFIX/var/log/dinit/rabbitmq-server.log

@include-opt $PREFIX/etc/dinit.d/config/rabbitmq-server.override
